// https://codeforces.com/group/MWSDmqGsZm/contest/219774/problem/D

#include <stdio.h>
 
int main() {
    int N;
    scanf("%d", & N);
    long long int arr[N];
    for (int i = 0; i <= N-1; i++) {
        scanf("%lld", &arr[i]);
    }
    for (int i = 0; i <= N-1; i++) {
        if (arr[i] <= 10) {
            printf("A[%d] = %d\n", i, arr[i] );
        }
    }
 
    return 0;
}
